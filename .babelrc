{
  "presets":["es2015","stage-0"],
  "plugins":[
    ["resolver", {
      "resolveDirs": ["src"]
    }],
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["babel-plugin-transform-runtime" ,{
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }]
  ]
}
